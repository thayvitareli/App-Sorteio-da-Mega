<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Main"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/primary"
    >

    <androidx.cardview.widget.CardView
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/cardview_compat_inset_shadow"
        android:layout_margin="5sp"
        >


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="12dp"
            >

            <TextView
                android:id="@+id/id_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:text=""
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/first_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="37dp"
                tools:layout_conversion_absoluteWidth="41dp" />

            <TextView
                android:id="@+id/first_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="n1"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/second_text"
                app:layout_constraintStart_toEndOf="@+id/id_text"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_conversion_absoluteHeight="32dp"
                tools:layout_conversion_absoluteWidth="48dp" />

            <TextView
                android:id="@+id/second_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="47dp"
                android:text="n2"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/sixth_text"
                app:layout_constraintStart_toEndOf="@+id/first_text"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_conversion_absoluteHeight="32dp"
                tools:layout_conversion_absoluteWidth="44dp" />

            <TextView
                android:id="@+id/sixth_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="n6"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/fourth_text"
                app:layout_constraintStart_toEndOf="@+id/second_text"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="48dp"
                tools:layout_conversion_absoluteWidth="42dp" />

            <TextView
                android:id="@+id/fourth_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="n4"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/fifth_text"
                app:layout_constraintStart_toEndOf="@+id/sixth_text"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_conversion_absoluteHeight="36dp"
                tools:layout_conversion_absoluteWidth="47dp" />

            <TextView
                android:id="@+id/fifth_text"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="68dp"
                android:text="n5"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/fourth_text"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_conversion_absoluteHeight="39dp"
                tools:layout_conversion_absoluteWidth="48dp" />

            <TextView
                android:id="@+id/third_text"
                android:layout_width="45dp"
                android:layout_height="wrap_content"
                android:text="n3"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/sixth_text"
                app:layout_constraintStart_toEndOf="@+id/second_text"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="34dp"
                tools:layout_conversion_absoluteWidth="47dp" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</LinearLayout>